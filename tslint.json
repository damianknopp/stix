{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "max-classes-per-file": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "variable-name": [
            true,
            "allow-leading-underscore",
            "allow-snake-case",
            "allow-pascal-case"
        ],
        "interface-name": false,
        "no-string-literal": false
    },
    "rulesDirectory": []
}